{
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true,
        "tabWidth": 3,
        "trailingComma": "es5",
        "endOfLine": "auto"
      }
    ],
    "@typescript-eslint/no-unused-vars": ["warn"],
    "no-console": "warn"
  }
}
